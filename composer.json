{
    "name": "superbrave/utility-package",
    "description": "A library with utility PHP classes.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "doctrine/dbal": "^2.9",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "SuperBrave\\UtilityPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SuperBrave\\UtilityPackage\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
